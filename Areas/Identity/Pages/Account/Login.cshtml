                <div class="one-left">Tax consultation and preparation with a </div>
                <div class="one-right">Human touch.</div>
                <div><input type="text" value="" class="form-control get-started-input" /></div>
                <div>
                    <button class="btn btn-primary get-started-btn ">Get Started</button>
                </div>

            </div>
            <div class="body-right top-margin">
                <img src="~/img/Data-Audit.png" class="data-audit" />
                <div class="loginFormBox">
                    <form id="account" method="post">
                        <div class="pnlLogintitle">PNL TAX CORP</div>
                        <div class="subtitle">
                            <div class="secured">
                                <div><img src="~/img/Secured.png" id="idsecured" /></div>
                                <div id="securedlbl">Secured</div>
                            </div>
                            <div class="advance">
                                <div><img src="~/img/Advance.png" id="idadvance" /></div>
                                <div id="advancelbl">Advance</div>
                            </div>
                        </div>
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="form-group">
                            <label asp-for="Input.Email"></label>
                            <input asp-for="Input.Email" class="form-control" />
                            <span asp-validation-for="Input.Email" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Input.Password"></label>
                            <input asp-for="Input.Password" class="form-control" />
                            <span asp-validation-for="Input.Password" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <div class="checkbox">

                                <p>
                                    <a id="forgot-password" asp-page="./ForgotPassword">Forgot your password?</a>
                                </p>

                            </div>
                        </div>
                        <div class="form-group">
                            <button type="submit" class="btn btn-primary loginbtn">Log in</button>
                            <p class="bysigning">
                                By clicking Sign in, you agree to our Terms and have
                                read and acknowledge our US Privacy Statement.
                            </p>
                        </div>
                        <div class="form-group remembercreatenew">
                            <label asp-for="Input.RememberMe">
                                <input asp-for="Input.RememberMe" />
                                @Html.DisplayNameFor(m => m.Input.RememberMe)
                            </label>
                            <p class="remembercreatenewBox">

                                <img src="~/img/user.png" class="usernewaccount" />
                                <a asp-page="./Register" asp-route-returnUrl="@Model.ReturnUrl">Create New Account</a>
                            </p>
                        </div>
                    </form>
                </div>
            </div>
        </div>
