services:
  db:
    container_name: db
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Express
      SA_PASSWORD: Password@123
    image: dioscarr/mssql:2019
    ports:
    - published: 1433
      target: 1433
  pnl:
    build:
      context: D:\Users\diosc\Repos\pnl
      dockerfile: Dockerfile
    container_name: app
    depends_on:
    - db
    environment:
      ASPNETCORE_ENVIRONMENT: Production
      ASPNETCORE_URLS: http://+:5000
    image: dioscarr/pnl:linux
    ports:
    - published: 5000
      target: 5000
    volumes:
    - C:\Users\Diosc\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Diosc\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'